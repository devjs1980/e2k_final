if ((word === "can" || word === "could") && nextWord) {

  const kreyolModal = dictionary.modal_verbs[word]; // ka / te ka
  const isQuestion = (i === 0);                     // premye mo = eske
  const subject = dictionary.pronouns[nextWord] || nextWord;

  if (isQuestion) {
    translated = `eske ${subject} ${kreyolModal}`;
  } else {
    translated = `${subject} ${kreyolModal}`;
  }

  i++;              // skip subject
  // result.push(translated);
  continue;         // ‚úÖ OBLIGATWA! Anpeche l√≤t kondisyon tradui mo yo ank√≤
}

// ‚úÖ TRADWI ‚Äúdo‚Äù KI VIN APR√à CAN/COULD K√íM VERB
if (word === "do") {

  if (words[i - 1] === "can" || words[i - 1] === "could") {
    translated = "f√®";
